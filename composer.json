{
    "name": "getcandy/core",
    "description": "GetCandy E-Commerce",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
          "name": "GetCandy",
          "homepage": "https://getcandy.io/"
        }
    ],
    "autoload": {
        "psr-4": {
            "GetCandy\\": "src",
            "GetCandy\\Database\\Factories\\": "database/factories",
            "GetCandy\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GetCandy\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GetCandy\\GetCandyServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-activitylog": "^4.1",
        "laravel/scout": "^9.3",
        "cartalyst/converter": "^6.1",
        "yab/laravel-scout-mysql-driver": "^5.0",
        "kalnoy/nestedset": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    }
}
